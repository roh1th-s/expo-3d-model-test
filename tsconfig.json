{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"allowJs": true,
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"noImplicitAny": true,
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "three-modules.d.ts"]
}
